<Window x:Class="Column.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Column"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        mc:Ignorable="d"
        Title="ColumnChart" MinHeight="450" MinWidth="800"
        Icon="/resources/logoColumnDiagram.png">
    <Grid>
        <lvc:CartesianChart x:Name="ChartColumn">
            <lvc:CartesianChart.AxisY>
                <lvc:Axis Title="Points" />
            </lvc:CartesianChart.AxisY>
        </lvc:CartesianChart>
    </Grid>
</Window>
